<div class="content outer portfolio">
  <div class="sidescroll middle">
    <ul class="images inner">
      {{#each data.images}}
      <li><img src="{{../data.imagesFolder}}{{this}}" alt=""></li>
      {{else}}
      <p class="empty">No content</p>
      {{/each}}
      <!-- access to parent's property with "../" -->
    </ul><!-- ul.images.inner -->
  </div><!-- div.sidescroll.middle -->
</div><!-- div.content.outer -->